          <ixz>1e-06</ixz>
        </inertia>
      </inertial>

      <!-- Visual -->
      <visual name="base_link_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://becard_base/meshes/base_link.stl</uri>
            <scale>0.1 0.1 0.1</scale>
